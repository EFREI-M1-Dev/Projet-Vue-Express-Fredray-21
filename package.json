{
  "name": "ExpressChat",
  "scripts": {
    "setup": "npm install && (cd front && npm install) && (cd .. && cd back && npm install)",
    "start-front": "cd front && npm run dev",
    "start-back": "cd back && npm run dev",
    "start": "concurrently \"npm run start-front\" \"npm run start-back\""
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
