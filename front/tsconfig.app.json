{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "front/src/**/*",
    "front/src/**/*.vue"
  ],
  "exclude": [
    "front/src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
